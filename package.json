{
  "name": "emaily-v2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "scripts": {
    "server-dev": "nodemon --exec babel-node bin/dev",
    "client-dev": "npm start --prefix client",
    "dev": "concurrently --kill-others-on-fail \"npm run server-dev\" \"npm run client-dev\"",
    "clean": "if [ -d dist ]; then rm -r dist; fi && mkdir -p dist",
    "server-build": "npm run clean && babel -d ./dist ./src",
    "client-build": "npm install --prefix client && npm run build --prefix client",
    "build": "npm run client-build && npm run server-build",
    "start": "node dist/index.js",
    "test": "npm run build && export NODE_ENV=test && node dist/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.3.3",
    "express": "^4.17.1",
    "mongoose": "^5.7.0",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "stripe": "^7.9.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.19.2",
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-react": "^7.0.0"
  }
}
